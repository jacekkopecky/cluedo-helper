  "bugs": {
    "url": "https://github.com/jacekkopecky/cluedo-helper/issues"
  },
  "homepage": "https://github.com/jacekkopecky/cluedo-helper#readme",
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.29.0",
    "parcel": "^2.15.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1"
  }
}
